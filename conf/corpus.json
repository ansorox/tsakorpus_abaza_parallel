{
  "corpus_name": "abaza_rus_parallel",
  "citation": "Anna Sorokina. Abaza-Russian Parallel Corpus. 2019. Moscow: School of Linguistics, HSE University. (Available online at https://linghub.ru/abaza_rus_corpus/search)",
  "input_format": "json-gzip",
  "debug": false,
  "media": false,
  "media_length": 60,
  "max_context_expand": 4,
  "max_distance_filter": 200000,
  "max_docs_retrieve": 9999,
  "max_words_in_sentence": 40,
  "query_timeout": 60,
  "viewable_meta": [
    "title",
    "title_abq",
    "author",
    "genre",
    "subgenre",
    "year"
  ],
  "sentence_meta": [  ],
  "search_meta": {
    "columns": [
      [
        {"type": "input_text", "name": "author", "placeholder": "Author"},
        {"type": "input_text", "name": "title", "placeholder": "Title"},
        {"type": "separator"},
        {"type": "input_number", "name": "year", "placeholder": "Year"}
      ],
      [
        {"type": "input_text", "name": "genre", "placeholder": "Genre"},
        {"type": "option", "name": "genre", "value": "fiction", "text": "fiction"},
        {"type": "option", "name": "genre", "value": "nonfiction", "text": "non-fiction"},
        {"type": "option", "name": "genre", "value": "poetry", "text": "poetry"}
      ],
      [
        {"type": "input_text", "name": "subgenre", "placeholder": "Subgenre"},
        {"type": "option", "name": "subgenre", "value": "children's", "text": "children's"},
        {"type": "option", "name": "subgenre", "value": "essay", "text": "essay"},
        {"type": "option", "name": "subgenre", "value": "play", "text": "play"},
        {"type": "option", "name": "subgenre", "value": "press", "text": "press"},
        {"type": "option", "name": "subgenre", "value": "story", "text": "story"}
      ]
    ],
    "stat_options": [
    ]
  },
  "word_fields": [],
  "languages": ["abaza", "russian"],
  "transliterations": [],
  "input_methods": ["normal", "strict"],
  "interface_languages": {"ru": "Russian"},
  "default_locale": "ru",
  "all_language_search_enabled": true,
  "gloss_search_enabled": false,
  "word_search_display_gr": false,
  "ambiguous_analyses": false,
  "lang_props": {"abaza": {}, "russian": {}}, 
  "sample_size": 1
}