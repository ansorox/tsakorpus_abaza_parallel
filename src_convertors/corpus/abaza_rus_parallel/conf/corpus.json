{
  "corpus_name": "abaza_rus_corpus",
  "input_format": "json-gzip",
  "debug": false,
  "media": false,
  "media_length": 60,
  "max_context_expand": 4,
  "max_distance_filter": 200000,
  "max_docs_retrieve": 9999,
  "max_words_in_sentence": 40,
  "query_timeout": 60,
  "viewable_meta": [
    "filename",
    "author",
    "title",
    "title_abq",
    "year"
  ],
  "sentence_meta": [  ],
  "search_meta": {
    "columns": [
      [
        {"type": "input_text", "name": "author", "placeholder": "Author"},
        {"type": "input_text", "name": "title", "placeholder": "Title"},
        {"type": "separator"},
        {"type": "input_number", "name": "year", "placeholder": "Year"}
      ],
      [
        {"type": "input_text", "name": "genre", "placeholder": "Genre"},
        {"type": "option", "name": "genre", "value": "fiction", "text": "fiction"},
        {"type": "option", "name": "genre", "value": "nonfiction", "text": "non-fiction"},
        {"type": "option", "name": "genre", "value": "press", "text": "press"},
        {"type": "option", "name": "genre", "value": "blog", "text": "blog"}
      ]
    ],
    "stat_options": [
      "genre",
      "author",
      "title",
      "year"
    ]
  },
  "word_fields":
  [
    "trans_ru",
    "trans_en"
  ],
  "languages": ["abaza", "russian"],
  "transliterations": [],

  "input_methods": ["normal","strict"],
  "interface_languages": {"en": "English", "ru": "Russian"},
  "all_language_search_enabled": true,
  "gloss_search_enabled": false,
  "ambiguous_analyses": true,
  "lang_props":
  {
    "abaza": {
      "gloss_shortcuts": {},
      "gr_fields_order": [],
      "gloss_selection": {
        "columns": [
        [
        ]
        ]
        },
      "gramm_selection": {
        "columns": [
          [
          ]
        ]
      }
    },
    "russian": {
      "gloss_shortcuts": {      },
      "gr_fields_order": [],
      "gloss_selection": {
        "columns": [
          [
          ]
        ]
      },
      "gramm_selection": {
        "columns": [
          [
          ]
        ]
      }
    }
  }
}